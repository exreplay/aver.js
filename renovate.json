{
  "extends": [
    "config:base",
    "group:allNonMajor",
    ":semanticCommitTypeAll(chore)"
  ],
  "rangeStrategy": "bump",
  "lockFileMaintenance": {
    "enabled": true
  },
  "ignorePaths": [
    "packages/@averjs/vue-app/lib"
  ],
  "ignoreDeps": [
    "core-js",
    "memory-fs",
    "postcss",
    "postcss-loader",
    "postcss-import",
    "sass-loader",
    "html-webpack-plugin",
    "copy-webpack-plugin",
    "terser-webpack-plugin",
    "@types/terser-webpack-plugin"
  ]
}
