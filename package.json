{
  "name": "averjs",
  "private": true,
  "workspaces": [
    "packages/@averjs/*",
    "packages/test"
  ],
  "scripts": {
    "rm-dist": "find ./packages/@averjs -name 'dist' -maxdepth 2 -exec rm -Rf {} \\;",
    "dev": "ts-node --project tsconfig.scripts.json ./scripts/dev --watch",
    "build": "ts-node --project tsconfig.scripts.json ./scripts/dev",
    "release": "ts-node --project tsconfig.scripts.json ./scripts/release-script",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "jest --runInBand && codecov",
    "version": "ts-node --project tsconfig.scripts.json ./scripts/generateChangelog && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.14.0",
    "@rollup/plugin-json": "4.1.0",
    "@types/conventional-recommended-bump": "6.0.0",
    "@types/expect-puppeteer": "4.4.5",
    "@types/inquirer": "7.3.1",
    "@types/jest-environment-puppeteer": "4.4.1",
    "@types/pify": "5.0.0",
    "@types/puppeteer": "5.4.3",
    "@types/semver": "7.3.4",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "codecov": "3.8.1",
    "cross-spawn": "7.0.3",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-lodash-template": "0.19.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "7.9.0",
    "esm": "3.2.25",
    "execa": "5.0.0",
    "inquirer": "8.0.0",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "jest-puppeteer": "5.0.2",
    "lerna": "4.0.0",
    "lerna-changelog": "1.0.1",
    "memfs": "3.2.2",
    "ora": "5.4.0",
    "package-json": "6.5.0",
    "pify": "5.0.0",
    "precss": "4.0.0",
    "prettier": "2.2.1",
    "puppeteer": "10.1.0",
    "rollup": "2.45.2",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-license": "2.3.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-jest": "26.5.5",
    "ts-node": "9.1.1",
